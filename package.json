    },
    "main": "dist-main/index.js",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "lint": "eslint . --ext js,ts,tsx",
        "package:publish": "electron-builder --config electron-builder.config.js --publish always",
        "package": "electron-builder --config electron-builder.config.js",
        "postinstall": "npm run rebuild",
        "prettier:check": "prettier --check src/**/*",
        "prettier:write": "prettier --write src/**/*",
        "rebuild": "electron-builder install-app-deps",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest",
        "test": "vitest run",
        "typecheck:main": "tsc -p src/main/tsconfig.json --noEmit",
        "typecheck:preload": "tsc -p src/preload/tsconfig.json --noEmit",
        "typecheck:renderer": "tsc -p src/renderer/tsconfig.json --noEmit",
        "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.11",
        "@types/color": "^3.0.6",
        "@types/node": "^20.8.6",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^7.16.0",
        "@typescript-eslint/parser": "^7.16.0",
        "@vitejs/plugin-react": "^4.3.1",
        "@vitest/coverage-v8": "^2.0.1",
        "electron": "^31.1.0",
        "electron-builder": "^24.13.3",
        "eslint": "^8.57.0",
        "node-abi": "^3.65.0",
        "prettier": "^3.3.2",
        "prettier-plugin-organize-imports": "^3.2.4",
        "tree-kill": "^1.2.2",
        "typescript": "^5.5.2",
        "typescript-eslint": "^7.16.0",
        "vite": "^5.3.3",
        "vite-plugin-electron": "^0.28.7",
        "vite-plugin-electron-renderer": "^0.14.5",
        "vitest": "^2.0.1"
    },
    "dependencies": {
        "@fluentui/react-components": "^9.54.4",
        "@fluentui/react-icons": "^2.0.249",
        "better-sqlite3": "^11.1.2",
        "color": "^4.2.3",
        "fuse.js": "^7.0.0",
        "fuzzysort": "^3.0.1",
        "i18next": "^23.8.1",
        "mathjs": "^13.0.2",
        "mitt": "^3.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^14.0.1",
        "react-router": "^6.21.3",
        "react-router-dom": "^6.21.3",
        "sharp": "^0.33.3"
    },
