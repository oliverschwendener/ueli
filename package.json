{
    "name": "ueli",
    "description": "Keystroke launcher for Windows, macOS and Linux",
    "version": "9.0.0-alpha.10",
    "license": "MIT",
    "author": {
        "email": "oliver.schwendener@proton.me",
        "name": "Oliver Schwendener",
        "url": "https://github.com/oliverschwendener"
    },
    "main": "dist-main/index.js",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "lint": "eslint . --ext js,ts,tsx",
        "package:publish": "electron-builder --config electron-builder.config.js --publish always",
        "package": "electron-builder --config electron-builder.config.js",
        "postinstall": "pnpm run rebuild",
        "prettier:check": "prettier --check src/**/*",
        "rebuild": "electron-rebuild rebuild.js",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest",
        "test": "vitest run",
        "typecheck:main": "tsc -p src/main/tsconfig.json --noEmit",
        "typecheck:preload": "tsc -p src/preload/tsconfig.json --noEmit",
        "typecheck:renderer": "tsc -p src/renderer/tsconfig.json --noEmit",
        "typecheck": "pnpm typecheck:main && pnpm typecheck:preload && pnpm typecheck:renderer"
    },
    "devDependencies": {
        "@electron/rebuild": "^3.6.0",
        "@types/better-sqlite3": "^7.6.9",
        "@types/node": "^20.8.6",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/coverage-v8": "^1.4.0",
        "electron": "^29.2.0",
        "electron-builder": "^24.13.3",
        "eslint": "^8.56.0",
        "prettier": "^3.2.4",
        "prettier-plugin-organize-imports": "^3.2.4",
        "tree-kill": "^1.2.2",
        "typescript": "^5.1.3",
        "vite": "^5.2.8",
        "vite-plugin-electron": "^0.28.4",
        "vite-plugin-electron-renderer": "^0.14.5",
        "vitest": "^1.4.0"
    },
    "dependencies": {
        "@fluentui/react-components": "^9.47.3",
        "@fluentui/react-icons": "^2.0.233",
        "better-sqlite3": "^9.5.0",
        "fuse.js": "^7.0.0",
        "i18next": "^23.8.1",
        "mathjs": "^12.3.1",
        "mitt": "^3.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^14.0.1",
        "react-router": "^6.21.3",
        "react-router-dom": "^6.21.3"
    }
}
