    },
    "main": "dist-main/index.js",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "package:publish": "electron-builder --config electron-builder.config.js --publish always",
        "package": "electron-builder --config electron-builder.config.js",
        "postinstall": "npm run rebuild",
        "prettier:check": "prettier --check src/**/*",
        "prettier:write": "prettier --write src/**/*",
        "rebuild": "electron-builder install-app-deps",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest",
        "test": "vitest run",
        "typecheck:main": "tsc -p src/main/tsconfig.json --noEmit",
        "typecheck:preload": "tsc -p src/preload/tsconfig.json --noEmit",
        "typecheck:renderer": "tsc -p src/renderer/tsconfig.json --noEmit",
        "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@types/better-sqlite3": "^7.6.11",
        "@types/color": "^3.0.6",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^20.8.6",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-v8": "^2.1.6",
        "electron": "^33.0.1",
        "electron-builder": "^25.1.8",
        "eslint": "^9.9.1",
        "globals": "^15.9.0",
        "node-abi": "^3.71.0",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.0.0",
        "tree-kill": "^1.2.2",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.13.0",
        "vite": "^6.0.0",
        "vite-plugin-electron": "^0.29.0",
        "vite-plugin-electron-renderer": "^0.14.6",
        "vitest": "^2.1.6"
    },
    "dependencies": {
        "@fluentui/react-components": "^9.54.4",
        "@fluentui/react-icons": "^2.0.249",
        "better-sqlite3": "^11.4.0",
        "color": "^4.2.3",
        "fast-xml-parser": "^4.5.0",
        "fuse.js": "^7.0.0",
        "fuzzysort": "^3.0.1",
        "i18next": "^23.8.1",
        "mathjs": "^13.1.1",
        "mitt": "^3.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^14.0.1",
        "react-router": "^6.21.3",
        "react-router-dom": "^6.21.3",
        "sharp": "^0.33.5",
        "uuid": "^11.0.3"
    }
}
