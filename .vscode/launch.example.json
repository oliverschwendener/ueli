{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Electron Main",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "program": "${workspaceRoot}/src/main/main.ts",
      "protocol": "inspector",
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "outFiles": [
        "${workspaceRoot}/bundle/main.js"
      ]
    }
  ]
}